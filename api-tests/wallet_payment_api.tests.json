							"        return rest;",
							"    });",
							"    const expectedCardFormFieldsDataWithoutSrc =",
							"     [",
							"            {",
							"                \"type\": \"TEXT\",",
							"                \"class\": \"CARD_FIELD\",",
							"                \"id\": \"CARD_NUMBER\"",
							"            },",
							"            {",
							"                \"type\": \"TEXT\",",
							"                \"class\": \"CARD_FIELD\",",
							"                \"id\": \"EXPIRATION_DATE\"",
							"            },",
							"            {",
							"                \"type\": \"TEXT\",",
							"                \"class\": \"CARD_FIELD\",",
							"                \"id\": \"SECURITY_CODE\"",
							"            },",
							"            {",
							"                \"type\": \"TEXT\",",
							"                \"class\": \"CARD_FIELD\",",
							"                \"id\": \"CARDHOLDER_NAME\"",
							"            }",
							"        ];",
